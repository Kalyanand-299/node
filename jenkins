pipeline {
    triggers {
        cron ('0 16 * * *')
     }
    agent {label 'JDK11'} 
    stages {
        stage('clone') { 
            steps {
                git url:'https://github.com/Kalyanand-299/node.git' , branch : 'main' 
            }
        }
        stage('install') {
            steps { 

                sh 'mvn package'
            }
        }
    }
}